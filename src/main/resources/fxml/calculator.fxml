<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="218.0" prefWidth="219.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <children>
      <Button layoutX="35.0" layoutY="117.0" mnemonicParsing="false" onAction="#multiply" prefHeight="25.0" prefWidth="33.0" text="*">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Button>
      <Button layoutX="75.0" layoutY="117.0" mnemonicParsing="false" onAction="#divide" prefHeight="25.0" prefWidth="33.0" text="/" />
      <Button layoutX="122.0" layoutY="117.0" mnemonicParsing="false" onAction="#sum" prefHeight="25.0" prefWidth="33.0" text="+" />
      <Button layoutX="162.0" layoutY="117.0" mnemonicParsing="false" onAction="#decrease" prefHeight="25.0" prefWidth="33.0" text="-" />
      <TextField fx:id="rightNumber" layoutX="35.0" layoutY="76.0" prefHeight="25.0" prefWidth="73.0">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </TextField>
      <Label fx:id="resultLabel" layoutX="93.0" layoutY="39.0" prefHeight="25.0" prefWidth="33.0">
         <font>
            <Font name="Arial Black" size="15.0" />
         </font>
      </Label>
      <TextField fx:id="leftNumber" layoutX="119.0" layoutY="76.0" prefHeight="25.0" prefWidth="73.0">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </TextField>
      <Button layoutX="83.0" layoutY="157.0" mnemonicParsing="false" onAction="#clear" prefHeight="25.0" prefWidth="50.0" text="Clear" />
   </children>
</AnchorPane>
